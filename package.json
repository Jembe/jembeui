{
  "scripts": {
    "dev_js": "parcel watch jembeui/src/js/jembeui.js --dist-dir jembeui/static/js/ --public-url ./",
    "build_js": "parcel build jembeui/src/js/jembeui.js --dist-dir jembeui/static/js/ --public-url ./",
    "dev_css": "export NODE_PATH=./node_modules;npx postcss jembeui/src/css/main.css -o jembeui/static/css/main.css --watch",
    "build_css": "export NODE_ENV=production;export NODE_PATH=./node_modules;npx postcss jembeui/src/css/main.css -o jembeui/static/css/main.css",
    "dev": "concurrently \"npm run dev_css\" \"npm run dev_js\"",
    "build": "concurrently \"npm run build_css\" \"npm run build_js\""
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.4.0",
    "cssnano": "^5.1.13",
    "daisyui": "^2.24.0",
    "nprogress": "^0.2.0",
    "parcel": "^2.7.0",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.1.8"
  }
}
